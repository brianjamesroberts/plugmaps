<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="unfairtools.com.plugmaps.UI.MainActivity"
    tools:showIn="@layout/app_bar_main">


    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="0dp"
            android:background="#fafafa"
            android:layout_marginLeft="14dp"
            android:layout_marginRight="14dp"
            android:layout_marginTop="10dp"
            android:id="@+id/searchbar_framelayout">
                <android.support.v7.widget.Toolbar
                    android:layout_height="50dp"
                    android:layout_width="wrap_content"
                    android:background="#dd55ff"
                    android:id="@+id/custom_toolbar"

                    >
                    <!--android:background="#FFbb66"-->

                </android.support.v7.widget.Toolbar>
            <LinearLayout
                android:paddingTop="20dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:id="@+id/toolbar_filter_options"
                android:visibility="gone">

                <ImageButton
                    android:id="@+id/image_button_select_car"
                    android:transitionName="svg_transition_car"
                    android:layout_width="160dp"
                    android:layout_height="80dp"
                    android:layout_gravity="center"
                    android:src="@drawable/ic_tesla_model_s"/>

            </LinearLayout>
        </LinearLayout>
</FrameLayout>
